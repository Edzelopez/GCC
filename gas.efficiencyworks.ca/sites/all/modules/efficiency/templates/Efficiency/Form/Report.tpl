{* Search panel for Gcc Report Search *}{if $searchDisplay}<fieldset><table>{foreach from=$formFields item=fieldArr key=fieldName}{if ($fieldName eq 'audit_completed') or ($fieldName eq 'retrofit_completed') or ($fieldName eq 'retrofit_invoiced') or ($fieldName eq 'audit_invoiced')}    {assign var="fieldDateFrom" value=$fieldName|cat:"_low"}    {assign var="fieldDateTo" value=$fieldName|cat:"_high"}        <tr>             <td class="label">                <span class="label">{$form.$fieldDateFrom.label}</span>            </td>             <td>              <span class="value">{include file="CRM/common/jcalendar.tpl" elementName=$fieldDateFrom}</span>            </td>                      <td>                 <span class="label"> {$form.$fieldDateTo.label}</span>            </td>            <td> <span class="value">{include file="CRM/common/jcalendar.tpl" elementName=$fieldDateTo}</span>            </td>         </tr>{else}        <tr>             <td class="label">                 {$form.$fieldName.label}            </td>             <td colspan="3">                {$form.$fieldName.html}            </td>         </tr>{/if}{/foreach}</table>{* Changed *}{if $isgas}   <div class="crm-block crm-form-block crm-contact-custom-search-form-block">   	<table class="list-participants-status">   	       <tr>		<td>Show Records</td>   			 <td>	   			 	<table>					<tr>						<td>{$form.status_audit_assigned.html}{$form.status_audit_assigned.label}</td>   						<td>{$form.status_retrofit_pending.html}{$form.status_retrofit_pending.label}</td>   						<td>{$form.status_no_potential.html}{$form.status_no_potential.label}</td>   					</tr>   					<tr>			     						<td>{$form.status_close_participant_withdraw.html}{$form.status_close_participant_withdraw.label}</td>   						<td>{$form.status_retrofit_completed.html}{$form.status_retrofit_completed.label}</td>	   						<td>{$form.status_project_completed.html}{$form.status_project_completed.label}</td>	   					</tr>		    				</table>		     			</td> 		       		</tr>             </table>    </div>{/if}{* Changed *}<div id="crm-submit-buttons">    {$form.buttons.html}</div></fieldset>{/if}{if $rowDisplay and $rows}    <div class="messages status">      <dl>         <dt><img src="{$config->resourceBase}i/Inform.gif" alt="{ts}status{/ts}"/></dt>         <dd>{ts}{$count} records found.{/ts} </dd>      </dl>    </div>        <div class="form-item">        {strip}	<table dojoType="SortableTable" widgetId="testTable" headClass="fixedHeader" headerSortUpClass="selectedUp" headerSortDownClass="selectedDown" tbodyClass="scrollContent" enableMultipleSelect="true" enableAlternateRows="true" rowAlternateClass="alternateRow" cellpadding="0" cellspacing="0" border="0">	<thead>        <tr class="columnheader">            <th field="File-ID" dataType="String">{ts}File-ID{/ts}</th>            <th field="Client Name" dataType="String">{ts}Client Name{/ts}</th>            <th field="Address" dataType="String">{ts}Address{/ts}</th>            <th field="Status" dataType="String">{ts}Muni{/ts}</th>            <th datatype="html"></th>        </tr>	</thead>        <tbody>          {foreach from=$rows item=row}           <tr class="{cycle values="odd-row,even-row"}">	        <td>{$row.FileID}</td>		        <td>{$row.Name}</td>	        <td>{$row.Address}</td>	        <td>{$row.Muni}</td>	        <td>{$row.action}</td>           </tr>          {/foreach}        </tbody>        </table>        {/strip}    </div>{elseif $searchDone}<br/>    <div class="messages status">     <dl>        <dt><img src="{$config->resourceBase}i/Inform.gif" alt="{ts}status{/ts}"/></dt>        <dd>{ts}No matches found.{/ts} </dd>     </dl>    </div>    {/if}